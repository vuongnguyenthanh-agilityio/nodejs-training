{
  "name": "shopping-app",
  "version": "1.0.0",
  "description": "Management products for shopping",
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=development npm run apidoc && nodemon --exec babel-node ./src/index.js",
    "test": "NODE_ENV=test jest --watchAll --verbose --runInBand",
    "apidoc": "apidoc -i ./src/api/routes/ -o public/apidoc",
    "stop": "pkill --signal SIGINT myApp"
  },
  "keywords": [
    "npm"
  ],
  "apidoc": {
    "name": "Node Shopping API - Documentation",
    "template": {
      "forceLanguage": "en"
    }
  },
  "author": "Vuong Nguyen",
  "license": "ISC",
  "dependencies": {
    "aws-sdk": "^2.422.0",
    "bcrypt": "^3.0.5",
    "body-parser": "^1.18.3",
    "compression": "^1.7.4",
    "consign": "^0.1.6",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "helmet": "^3.16.0",
    "jwt-simple": "^0.5.5",
    "morgan": "^1.9.1",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "regenerator-runtime": "^0.13.2",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.4.0",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.4.2",
    "apidoc": "^0.17.7",
    "eslint": "^5.15.2",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "jest": "^24.5.0",
    "nodemon": "^1.18.10",
    "supertest": "^1.2.0"
  },
  "resolutions": {
    "nodemon/pstree.remy": "1.1.0"
  },
  "jest": {
    "globals": {
      "__DEV__": true,
      "__RCTProfileIsProfiling": false
    },
    "transform": {
      ".*": "<rootDir>/node_modules/babel-jest"
    }
  }
}
